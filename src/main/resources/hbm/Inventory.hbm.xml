<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping
	package="com.example.input.domain.misc.inventory">
	<class name="Inventory" table="inventory">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<many-to-one name="part" column="part_id" />
		<property name="actual" column="actual" type="integer"></property>
		<property name="location" column="location_id" type="integer"></property>
	</class>
</hibernate-mapping>