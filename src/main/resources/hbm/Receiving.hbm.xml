<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping
	package="com.example.input.domain.misc.receiving">
	<class name="receiving" table="receiving">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<many-to-one name="part" column="part_id"></many-to-one>
		<property name="quantity" column="quantity" type="string"></property>
		<many-to-one name="quantityType" column="quantity_type_id" />
		<many-to-one name="employee" column="employee_id" />
		<property name="receptionDay" column="reception_day"></property>
	</class>
</hibernate-mapping>