<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping
	package="com.example.input.domain.misc.admin">
	<class name="Permission" table="permissions">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="admin" column="admin" type="boolean"></property>
		<property name="inventory" column="inventory" type="boolean"></property>
		<property name="reception" column="reception" type="boolean"></property>
		<property name="buyer" column="buyer" type="boolean"></property>
	</class>
</hibernate-mapping>

